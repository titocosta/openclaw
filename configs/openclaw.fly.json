{
  "agents": {
    "defaults": {
      "model": {
        "primary": "google/gemini-3-flash",
        "fallbacks": ["google/gemini-3-pro-preview", "anthropic/claude-sonnet-4-5"]
      },
      "maxConcurrent": 4
    },
    "list": [
      {
        "id": "main",
        "default": true
      }
    ]
  },
  "auth": {
    "profiles": {
      "anthropic:default": { "mode": "token", "provider": "anthropic" }
    }
  },
  "bindings": [
    {
      "agentId": "main",
      "match": { "channel": "discord" }
    },
    {
      "agentId": "main",
      "match": { "channel": "http-webhook" }
    }
  ],
  "channels": {
    "http-webhook": {
      "enabled": true,
      "inbound": {
        "port": 8080,
        "path": "/",
        "token": "${HTTP_WEBHOOK_INBOUND_TOKEN}"
      },
      "outbound": {
        "url": "${HTTP_WEBHOOK_OUTBOUND_URL}",
        "token": "${HTTP_WEBHOOK_OUTBOUND_TOKEN}",
        "timeoutSeconds": 30
      },
      "dm": {
        "policy": "open",
        "allowFrom": ["*"]
      }
    }
  },
  "plugins": {
    "allow": ["http-webhook"],
    "entries": {
      "http-webhook": {
        "enabled": true
      }
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "auto",
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": false
        }
      }
    }
  },
  "diagnostics": {
    "enabled": true
  },
  "meta": {
    "lastTouchedVersion": "2026.1.29"
  }
}
